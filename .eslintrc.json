{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules":{
    "linebreak-style": 0,
    "no-console":0,
    "no-shadow": [2, {"allow": ["done", "query"]}],
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-new": 0,
    "class-methods-use-this": 0,
    "no-unused-vars": 0,
    "consistent-return": 0,
    "no-restricted-syntax": 0,
    "no-else-return": 0,
    "func-names": 0,
    "no-use-before-define": 0,
    "one-var": 0,
    "prefer-destructuring": 0,
    "no-continue": 0
  },
  "globals": { 
    "mp": true,
    "player": true,
    "console": true
  }
}